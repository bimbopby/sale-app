<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape>
            <solid android:color="@color/zm_transparent" />
            <stroke android:width="1.0dip" android:color="@color/zm_v2_blue_text_color" />
            <padding android:left="8.0dip" android:top="8.0dip" android:right="8.0dip" android:bottom="8.0dip" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/zm_transparent" />
            <stroke android:width="1.0dip" android:color="@color/zm_v2_cell_divider" />
            <padding android:left="8.0dip" android:top="8.0dip" android:right="8.0dip" android:bottom="8.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/zm_highlight_disabled" />
            <padding android:left="8.0dip" android:top="8.0dip" android:right="8.0dip" android:bottom="8.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/zm_transparent" />
            <stroke android:width="1.0dip" android:color="@color/zm_v2_cell_divider" />
            <padding android:left="8.0dip" android:top="8.0dip" android:right="8.0dip" android:bottom="8.0dip" />
        </shape>
    </item>
</selector>